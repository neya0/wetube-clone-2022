mixin video(video)
    div.video-list
        video(src=`/${video.videoUrl}`, width=300, height=180, controls)
        div.video-descript
            h4
                a(href=`/videos/${video.id}`)=video.title
            p=video.description
            div.hashtag
                each hashtag in video.hashtags
                        li=hashtag
            small=video.createdAt